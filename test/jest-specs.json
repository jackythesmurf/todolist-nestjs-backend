{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testMatch": ["/**/*.spec.ts"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage",
  "collectCoverageFrom": ["src/**/*.(t|j)s", "!src/**/*.module.ts", "!src/**/main.ts"],
  "coveragePathIgnorePatterns": ["node_modules"]
}
